@page
@model ProyectoCiclo3.App.Frontend.Pages.FormServicioModel
@{
}
 
<div class="text-center">
    <form method=”POST”>
        <label for="origen" class="form-label">Remitente:</label>
        <input type="text" class="form-control" id="origen" name="origen" placeholder="Remitente">
        <label for="destino" class="form-label">Destinatario:</label>
        <input type="text" class="form-control" id="destino" name="destino" placeholder="destino">
        <label for="fecha" class="form-label">Fecha:</label>
        <input type="date" class="form-control" id="fecha" name="fecha" placeholder="Fecha">
        <label for="hora" class="form-label">Hora:</label>
        <input type="time" class="form-control" id="hora" name="hora" placeholder="Hora">
        <label for="encomienda" class="form-label"># Encomienda:</label>
        <input type="text" class="form-control" id="encomienda" name="encomienda" placeholder="Encomienda">
        
        <hr>
        <button type="submit" class="btn btn-success">Enviar</button>
    </form>
</div>